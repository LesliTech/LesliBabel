<p id="notice"><%= notice %></p>

<h1>Translation Modules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @translation_modules.each do |translation_module| %>
      <tr>
        <td><%= link_to 'Show', translation_module %></td>
        <td><%= link_to 'Edit', edit_translation_module_path(translation_module) %></td>
        <td><%= link_to 'Destroy', translation_module, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Translation Module', new_translation_module_path %>
