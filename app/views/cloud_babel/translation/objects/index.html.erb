<p id="notice"><%= notice %></p>

<h1>Translation Objects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @translation_objects.each do |translation_object| %>
      <tr>
        <td><%= link_to 'Show', translation_object %></td>
        <td><%= link_to 'Edit', edit_translation_object_path(translation_object) %></td>
        <td><%= link_to 'Destroy', translation_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Translation Object', new_translation_object_path %>
