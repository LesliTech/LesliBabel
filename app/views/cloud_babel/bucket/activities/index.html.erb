<p id="notice"><%= notice %></p>

<h1>Bucket Activities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bucket_activities.each do |bucket_activity| %>
      <tr>
        <td><%= link_to 'Show', bucket_activity %></td>
        <td><%= link_to 'Edit', edit_bucket_activity_path(bucket_activity) %></td>
        <td><%= link_to 'Destroy', bucket_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bucket Activity', new_bucket_activity_path %>
